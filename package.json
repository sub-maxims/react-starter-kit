{
  "name": "react-kit",
  "version": "0.0.2",
  "description": "A starter kit for React",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf __public__",
    "build": "npm run clean && NODE_ENV=production ./node_modules/.bin/webpack -p --config webpack/prod.config.js --progress --profile --colors",
    "stats": "./node_modules/.bin/webpack --config webpack/prod.config.js  --profile --json > ./stats.json",
    "postinstall": "npm run build",
    "dev": "NODE_ENV=development ./node_modules/.bin/nodemon app.js",
    "start": "NODE_ENV=production PORT=8080 node app.js"
  },
  "author": "Hendrik Kurniawan",
  "license": "ISC",
  "dependencies": {
    "history": "^1.13.1",
    "lodash": "^3.10.1",
    "normalize.css": "^3.0.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-router": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.3.21",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.13.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "http-proxy": "^1.12.0",
    "node-sass": "^3.4.2",
    "nodemon": "^1.8.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-dev-server": "^1.14.0"
  }
}
